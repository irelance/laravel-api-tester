{
    "name": "asvae/laravel-api-tester",
    "description": "Api tester for Laravel Framework",
    "keywords": ["laravel", "api", "debug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yauheni Prakopchyk",
            "email": "ontrew@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "laravel/framework": "5.6.*",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "suggest": {
        "ktamas77/firebase-php": "Keep your requests stored in cloud",
        "firebase/token-generator": "Token generator for firebase"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "Asvae\\ApiTester\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asvae\\ApiTester\\ServiceProvider"
            ]
        }
    }
}
